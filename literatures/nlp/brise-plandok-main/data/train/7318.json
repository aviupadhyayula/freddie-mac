ls_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {"PlangebietAllgemein": {"annotators": ["01"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7318_10_0": {"id": "7318_10_0", "text": "An den zu den \u00f6ffentlichen Verkehrsfl\u00e4chen gerichteten Fronten d\u00fcrfen die Baumassen der Geb\u00e4ude an den Baulinien nicht gestaffelt werden.", "gold_modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"AnFluchtlinie": {"name": "AnFluchtlinie", "value": null, "type": null}}, "annotated_attributes": {"AnFluchtlinie": {"annotators": ["01", "06"]}, "AnOeffentlichenVerkehrsflaechen": {"annotators": ["01"]}, "VerbotStaffelung": {"annotators": ["01", "06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7318_10_1": {"id": "7318_10_1", "text": "Vorstehende Bauelemente, die der Gliederung oder der architektonischen Ausgestaltung der Schauseiten dienen sowie Erker, Balkone und vorragende Loggien d\u00fcrfen an Seite - 3 - Stra\u00dfen bis 16 m Breite h\u00f6chstens 0,6 m und an Stra\u00dfen von mehr als 16 m Breite h\u00f6chstens 0,8 m \u00fcber die Baulinie ragen.", "gold_modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"VerkehrsflaecheID": {"name": "VerkehrsflaecheID", "value": null, "type": null}}, "annotated_attributes": {"StrassenbreiteMax": {"annotators": ["01", "06"]}, "StrassenbreiteMin": {"annotators": ["01", "06"]}, "VorstehendeBauelementeAusladungMax": {"annotators": ["01", "06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7318_11_0": {"id": "7318_11_0", "text": "Bei den innerhalb des Baulandes zur Errichtung gelangenden Geb\u00e4uden darf der h\u00f6chste Punkt des Daches nicht h\u00f6her als 4,5 m \u00fcber der ausgef\u00fchrten Geb\u00e4udeh\u00f6he liegen.", "gold_modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"AbschlussDachMaxBezugGebaeude": {"name": "AbschlussDachMaxBezugGebaeude", "value": null, "type": null}}, "annotated_attributes": {"AbschlussDachMaxBezugGebaeude": {"annotators": ["01", "06"]}, "GebaeudeHoeheArt": {"annotators": ["01", "06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7318_12_0": {"id": "7318_12_0", "text": "Die D\u00e4cher der auf den mit der Festsetzung GV I 4 , 5 m bzw. 5,5 m g bezeichneten Grundfl\u00e4chen zur Errichtung gelangenden Geb\u00e4ude sind entsprechend dem Stand der Technik als begr\u00fcnte Flachd\u00e4cher auszubilden, sofern es sich nicht um Glasdachkonstruktionen handelt.", "gold_modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"BegruenungDach": {"name": "BegruenungDach", "value": null, "type": null}, "Dachart": {"name": "Dachart", "value": null, "type": null}}, "annotated_attributes": {"BauweiseID": {"annotators": ["01", "06"]}, "BegruenungDach": {"annotators": ["01", "06"]}, "Dachart": {"annotators": ["01", "06"]}, "Planzeichen": {"annotators": ["01"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7318_12_1": {"id": "7318_12_1", "text": "F\u00fcr alle \u00fcbrigen Geb\u00e4ude bis zu einer Geb\u00e4udeh\u00f6he von 7,5 m wird bestimmt, dass Flachd\u00e4cher bis zu einer Dachneigung von", "gold_modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"GebaeudeHoeheMax": {"name": "GebaeudeHoeheMax", "value": null, "type": null}}, "annotated_attributes": {"GebaeudeHoeheMax": {"annotators": ["06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7318_13_0": {"id": "7318_13_0", "text": "Grad entsprechend dem Stand der Technik zu begr\u00fcnen sind.", "gold_modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"BegruenungDach": {"name": "BegruenungDach", "value": null, "type": null}}, "annotated_attributes": {"BegruenungDach": {"annotators": ["06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7318_13_1": {"id": "7318_13_1", "text": "Technische bzw. der Belichtung dienende Aufbauten sind im erforderlichen Ausma\u00df zul\u00e4ssig.", "gold_modality": null, "already_gold_on_annotation": true, "labels_gold_exists": true, "gold_attributes": {"AufbautenZulaessig": {"name": "AufbautenZulaessig", "type": null, "value": null}}, "gen_attributes_on_annotation": {"AufbautenZulaessig": {"name": "AufbautenZulaessig", "value": null, "type": null}}, "annotated_attributes": {"AufbautenZulaessig": {"annotators": ["01", "06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7318_14_0": {"id": "7318_14_0", "text": "Nicht bebaute, jedoch bebaubare Baulandfl\u00e4chen sind g\u00e4rtnerisch auszugestalten.", "gold_modality": null, "already_gold_on_annotation": false, "labels_gold_exists": true, "gold_attributes": {"AnordnungGaertnerischeAusgestaltung": {"name": "AnordnungGaertnerischeAusgestaltung", "type": null, "value": null}}, "gen_attributes_on_annotation": {"AnordnungGaertnerischeAusgestaltung": {"name": "AnordnungGaertnerischeAusgestaltung", "value": null, "type": null}}, "annotated_attributes": {"AnordnungGaertnerischeAusgestaltung": {"annotators": ["01", "06"]}, "Flaechen": {"annota