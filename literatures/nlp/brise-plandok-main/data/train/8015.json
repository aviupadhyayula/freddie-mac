_attributes": {}, "gen_attributes_on_annotation": {"Planzeichen": {"name": "Planzeichen", "value": null, "type": null}}, "annotated_attributes": {"DurchgangHoehe": {"annotators": ["05", "06"]}, "Planzeichen": {"annotators": ["05", "06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "8015_17_1": {"id": "8015_17_1", "text": "Der Abteilungsleiter: (interim.) Dipl.-Ing. Walter Krauss Senatsrat", "gold_modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}}, "labels_gold": false, "annotators": ["05", "06"], "full_gold": false}