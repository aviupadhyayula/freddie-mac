l}, "Bauklasse": {"name": "Bauklasse", "type": null, "value": null}, "GebaeudeHoeheMax": {"name": "GebaeudeHoeheMax", "type": null, "value": null}}, "gen_attributes_on_annotation": {"BegruenungDach": {"name": "BegruenungDach", "value": null, "type": null}, "Dachart": {"name": "Dachart", "value": null, "type": null}, "GebaeudeHoeheMax": {"name": "GebaeudeHoeheMax", "value": null, "type": null}}, "annotated_attributes": {"BegruenungDach": {"annotators": ["02", "06"]}, "Dachart": {"annotators": ["02", "06"]}, "GebaeudeHoeheMax": {"annotators": ["02", "06"]}, "WidmungUndZweckbestimmung": {"annotators": ["02"]}, "Bauklasse": {"annotators": ["06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7426_11_1": {"id": "7426_11_1", "text": "Technische bzw. der Belichtung dienende Aufbauten sind im erforderlichen Ausma\u00df zul\u00e4ssig.", "gold_modality": null, "already_gold_on_annotation": true, "labels_gold_exists": true, "gold_attributes": {"AufbautenZulaessig": {"name": "AufbautenZulaessig", "type": null, "value": null}}, "gen_attributes_on_annotation": {"AufbautenZulaessig": {"name": "AufbautenZulaessig", "value": null, "type": null}}, "annotated_attributes": {"AufbautenZulaessig": {"annotators": ["02", "06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7426_12_0": {"id": "7426_12_0", "text": "Einfriedungen auf g\u00e4rtnerisch auszugestaltenden Fl\u00e4chen d\u00fcrfen 2 m nicht \u00fcberragen und ab einer H\u00f6he von 0,5 m den freien Durchblick nicht hindern.", "gold_modality": null, "already_gold_on_annotation": false, "labels_gold_exists": true, "gold_attributes": {"EinfriedungHoeheGesamt": {"name": "EinfriedungHoeheGesamt", "type": null, "value": null}, "EinfriedungAusgestaltung": {"name": "EinfriedungAusgestaltung", "type": null, "value": null}, "AnordnungGaertnerischeAusgestaltung": {"name": "AnordnungGaertner