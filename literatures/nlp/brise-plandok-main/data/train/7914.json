": false, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {"Flaechen": {"annotators": ["04"]}, "Planzeichen": {"annotators": ["04"]}, "WidmungUndZweckbestimmung": {"annotators": ["04"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7914_25_1": {"id": "7914_25_1", "text": "Der Abteilungsleiter: Dipl.-Ing. Krauss Senatsrat", "gold_modality": null, "already_gold_on_annotation": true, "labels_gold_exists": true, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}}, "annotators": ["04"], "labels_gold": false, "full_gold": false}